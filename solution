  var num = window.prompt("enter a number");
    var originalNum=num;
    var reverse=0;

    while(num != 0){
        reverse= (reverse*10) + (num%10);
        num = parseInt(num/10)
    }
    if(originalNum==reverse){
            console.log("palindrome number")
        }else{
            console.log("not a palindrome number")
        }
